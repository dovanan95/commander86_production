        "express": "^4.18.1",
        "express-mongo-db": "^2.0.4",
        "fabric-ca-client": "^2.2.4",
        "fabric-network": "^2.2.11",
        "formidable": "^2.0.1",
        "jsonwebtoken": "^8.5.1",
        "mongodb": "^4.7.0",
        "mssql": "^8.1.2",
        "multer": "^1.4.4",
